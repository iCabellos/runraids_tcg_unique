# Django Configuration
SECRET_KEY=your-secret-key-here
DEBUG=True

# Database Configuration - REQUIRED for Supabase
# Get this from: Supabase Project > Settings > Database > Connection pooling
# Copy the "Connection string" and replace [YOUR-PASSWORD] with your actual password
DATABASE_URL=postgresql://postgres.your-project-ref:[YOUR-PASSWORD]@aws-0-us-east-1.pooler.supabase.com:6543/postgres

# Example:
# DATABASE_URL=postgresql://postgres.abcdefghijklmnop:mypassword123@aws-0-us-east-1.pooler.supabase.com:6543/postgres
