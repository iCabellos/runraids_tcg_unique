{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles_build/$1",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    { "src": "/(.*)", "dest": "/api/wsgi.py" }
  ]
}
