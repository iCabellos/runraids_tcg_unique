{
  "functions": {
    "api/server.py": {
      "runtime": "vercel-python@3.12",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles_build/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.py"
    }
  ]
}
