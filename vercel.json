{
  "functions": {
    "api/server.py": {
      "runtime": "python3.11",
      "maxDuration": 10
    }
  },
  "buildCommand": "bash build_files.sh",
  "outputDirectory": "staticfiles_build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles_build/$1",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.py"
    }
  ]
}
